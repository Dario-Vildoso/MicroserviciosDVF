<!DOCTYPE html>
<html>
<head>
  <title>Lista de Usuarios</title>
</head>
<body>
  <h1>Usuarios Registrados</h1>
  <a href="/add">Agregar Usuario</a>
  <table border="1" cellpadding="5">
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Correo Electr√≥nico</th>
      <th>Fecha de Registro</th>
      <th>Acciones</th>
    </tr>
    <% usuarios.forEach(u => { %>
      <tr>
        <td><%= u.id %></td>
        <td><%= u.nombre %></td>
        <td><%= u.correo %></td>
        <td><%= u.fecha_registro %></td>
        <td><a href="/delete/<%= u.id %>">Eliminar</a></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
